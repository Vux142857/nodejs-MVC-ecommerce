<% filterStatusHelper=(statusFilter, keyword)=> { %>
    <% statusFilter.forEach((filter)=> {
        let link = (keyword != '' && keyword != undefined) ? filter.value + "?search=" + keyword : filter.value;
        %>
        <a class="btn m-b-sm btn-default <%= filter.class %> btn-sm" href="<%= link %>">
            <%= filter.name %> (<%= filter.count %>)
        </a>
        <% }); %>
            <% } %>