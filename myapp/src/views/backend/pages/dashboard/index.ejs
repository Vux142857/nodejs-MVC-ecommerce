<section class="content">
  <div class="container-fluid">
    <div class="row"><%- include("./live-count") %></div>

    <div class="row">
      <section class="col-lg-7 connectedSortable ui-sortable">
        <%- include("./live-order") %>
      </section>
      <section class="col-lg-5 connectedSortable ui-sortable">
        <%- include("./chat-notify") %>
      </section>
    </div>
  </div>
</section>
<script>
  var socket = io();
  socket.on("userInArticle", function (data) {
    $("#articleCount").text(data.value);
  });
  socket.on("userInProduct", function (data) {
    $("#productCount").text(data.value);
  });
  socket.on("totalVistor", function (data) {
    $("#totalVistor").text(data.value);
  });
</script>
