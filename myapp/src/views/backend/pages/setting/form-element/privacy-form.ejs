<% privacyForm = () => {%>
<div class="form-group">
  <label for="privacy-copyright">Privacy Copyright:</label>
  <input
    class="form-control"
    type="text"
    id="privacy-copyright"
    name="copyright"
    value="<%= data.privacy.copyright %>"
  />
</div>
<div class="form-group">
  <label for=" privacy-doc"> Privacy Document:</label>
  <textarea class="form-control" name="doc" rows="2" id="content_ckeditor5">
  <%= data.privacy.doc %>
  </textarea>
</div>
<div class="form-group">
  <label for=" privacy-doc"> Help Document:</label>
  <textarea class="form-control" name="help" rows="10" id="content_ckeditor0">
  <%= data.privacy.help %>
    </textarea
  >
</div>
<%} %>
