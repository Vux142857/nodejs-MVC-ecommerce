<% statusHelper=(collection, status, id)=> { let statusClass = (status
==='active') ? "btn btn-block btn-info" : "btn btn-block btn-danger" ; %>

<a
  href="javascript:changeStatus('<%= id %>','<%= status %>','<%= collection %>')"
  class="<%= statusClass %>"
>
  <span class="label"> <%= status %> </span>
</a>
<% } %>
